---
import CategoryPosts from "../../layouts/CategoryPosts.astro";

export function getStaticPaths() {
  const categorias = [
    {
      id: "computing",
      name: "Computing",
    },
    {
      id: "desk",
      name: "Desk",
    },
    {
      id: "video",
      name: "Video",
    },
  ];
  return categorias.map(({ id, name }) => {
    return {
      params: { id },
      props: { name },
    };
  });
}
const { name } = Astro.props;
---

<CategoryPosts category={name} />
